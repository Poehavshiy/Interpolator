<Window x:Class="OneDemSPH.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneDemSPH"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="274*"/>
            <RowDefinition Height="45*"/>
        </Grid.RowDefinitions>
        <oxy:PlotView Model="{Binding Model1}" Grid.Row="0"/>
        <Button x:Name="button" Content="Click Me" HorizontalAlignment="Left" Height="45" VerticalAlignment="Top" Width="517" Click="button_Click" Grid.Row="1"/>

    </Grid>
</Window>
